<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - @Constants.APPLICATION_NAME</title>
    @* Css import render *@
    @await Component.InvokeAsync("StyleSheet")
</head>
<body>
    <section>
        <script src="~/js/store.js"></script>
        <script src="~/js/common.js"></script>
    </section>

    <main id="page-load">
        @RenderBody()
    </main>

    @* Script import render *@
    @await Component.InvokeAsync("Script")
    @RenderSection("Scripts", required: false)
</body>
</html>
