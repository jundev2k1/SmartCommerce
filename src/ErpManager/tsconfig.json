    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "rootDir": "./Scripts",
    "outDir": "wwwroot/js",
    "lib": [ "ES2022", "DOM" ]
  },
  "include": [
    "scripts/**/*"
  ]
}