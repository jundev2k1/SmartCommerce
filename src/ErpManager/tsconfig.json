{
  "compileOnSave": true,
  "compilerOptions": {
    "allowJs": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "es6",
    "target": "es5",
    "rootDir": "./Scripts",
    "outDir": "wwwroot/js",
    "lib": [ "ES2022", "DOM" ]
  },
  "include": [
    "scripts/**/*"
  ]
}